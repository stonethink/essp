<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

<class  name="db.essp.cbs.CbsCostLabor"  table="cbs_cost_labor" dynamic-update="true" dynamic-insert="true">
    <id name="rid" type="java.lang.Long" column="RID">
        <generator class="assigned" />
    </id>

    <property name="acntRid" type="java.lang.Long" column="ACNT_RID" length="8">
    </property>
    <property name="jobCodeId" type="java.lang.String" column="JOB_CODE_ID" length="50">
    </property>
    <property name="price" type="java.lang.Double" column="PRICE" length="10">
    </property>
    <property name="currency" type="java.lang.String" column="CURRENCY" length="50">
    </property>
    <property name="costUnit" type="java.lang.String" column="COST_UNIT" length="50">
    </property>
    <property name="costUnitNum" type="java.lang.Double" column="COST_UNIT_NUM" length="10">
    </property>
    <property name="costAmt" type="java.lang.Double" column="COST_AMT" length="12">
    </property>
    <property name="rst" type="java.lang.String" column="RST" length="1">
    </property>
    <property name="rct" type="java.util.Date" column="RCT" length="20">
    </property>
    <property name="rut" type="java.util.Date"  column="RUT" length="20">
    </property>

    <!-- Associations -->

</class>
</hibernate-mapping>
