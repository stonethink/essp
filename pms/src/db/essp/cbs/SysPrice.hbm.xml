<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
<class  name="db.essp.cbs.SysPrice"  table="sys_price" dynamic-update="true" dynamic-insert="true">
    <id  name="rid" type="java.lang.Long" column="RID">
        <generator class="assigned" />
    </id>

    <property name="acntRid" type="java.lang.Long" column="ACNT_RID" length="8"/>
    <property name="catalog" type="java.lang.String" column="CATALOG" length="50"/>
    <property name="id" type="java.lang.String" column="ID" length="50"/>
    <property name="name" type="java.lang.String" column="NAME" length="50"/>
    <property name="subjectCode" type="java.lang.String" column="SUBJECT_CODE" length="50"/>
    <property name="unit" type="java.lang.String" column="UNIT" length="50"/>
    <property name="currency" type="java.lang.String" column="CURRENCY" length="50"/>
    <property name="price" type="java.lang.Double" column="PRICE" length="12"/>
    <property name="priceScope" type="java.lang.String" column="PRICE_SCOPE" length="50"/>
    <property name="rst" type="java.lang.String" column="RST" length="1"/>
    <property name="rct"  type="java.util.Date" column="RCT"  length="20"/>
    <property name="rut" type="java.util.Date" column="RUT" length="20"/>

    <!-- Associations -->


</class>
</hibernate-mapping>
